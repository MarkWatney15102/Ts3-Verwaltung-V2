{
    "routes": {
        "login": {
          "request": "/login",
          "controller": "controller/LoginController/LoginController",
          "controllerName": "LoginController",
          "access": "allway-accessabel"
        },
        "logout": {
          "request": "/logout",
          "controller": "controller/LogoutController/LogoutController",
          "controllerName": "LogoutController"
        },
        "dashboard": {
          "request": "/",
          "secondRequest": "/home",
          "controller": "controller/HomeController/HomeController",
          "controllerName": "HomeController"
        },
        "accoutlist": {
          "request": "/admin-accountlist",
          "controller": "controller/Account/AccountList/AccountList",
          "controllerName": "AccountList"
        },
        "userprofile": {
          "request": "/admin-userprofile",
          "controller": "controller/Account/UserProfile/UserProfile",
          "controllerName": "UserProfile",
          "neededParams": {
            "1": "user_id"
          }
        },
        "clientProfile": {
          "request": "/client",
          "controller": "controller/Client/ClientProfile/ClientProfile",
          "controllerName": "ClientProfile",
          "neededParams": {
            "1": "client_uid"
          }
        },
        "notes": {
          "request": "/note",
          "controller": "controller/Client/Note/Note",
          "controllerName": "Note",
          "neededParams": {
            "1": "note_id"
          }
        },
        "createNote": {
          "request": "/create-note",
          "controller": "controller/Client/CreateNote/CreateNote",
          "controllerName": "CreateNote",
          "neededParams": {
            "1": "client_uid"
          }
        },
        "channel-list": {
          "request": "/channel-list",
          "controller": "controller/Channel/ChannelList/ChannelList",
          "controllerName": "ChannelList"
        }
    }
}
